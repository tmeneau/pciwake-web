{
  "compilerOptions": {
    "target": "es5",
    "module": "system",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "rootDir": "./src/ts",
    "outDir": "./dist/app"
  },
  "compileOnSave": false,
  "filesGlob": [
    "./src/app/**/*.ts",
    "./typings/**/*.d.ts",
    "./node_modules/ng2-bs3-modal/dist/**/*.d.ts"
  ],
  "exclude": [
    "node_modules",
    "node_modules/**/*",
    "./typings/main",
    "./typings/main/**/*",
    "./typings/main.d.ts"
  ],
  "files": [
    "./src/app/app.component.ts",
    "./src/app/components/dashboard/dashboard.component.ts",
    "./src/app/main.ts",
    "./src/app/shared/auth/auth.directive.ts",
    "./src/app/shared/auth/auth.service.ts",
    "./src/app/shared/auth/login.component.ts",
    "./src/app/shared/config/config.service.ts",
    "./src/app/shared/logmanager/graylog/graylog.interface.ts",
    "./src/app/shared/logmanager/graylog/graylogconfigview.component.ts",
    "./src/app/shared/logmanager/logmanager.interface.ts",
    "./src/app/shared/logmanager/logmanager.service.ts",
    "./src/app/shared/logmanager/managerconfigs.component.ts",
    "./src/app/shared/rest/http.overrides.ts",
    "./src/app/shared/rest/rest.interface.ts",
    "./src/app/shared/util/deserializable.ts",
    "./typings/browser.d.ts",
    "./typings/browser/ambient/es6-shim/es6-shim.d.ts",
    "./typings/browser/definitions/moment/moment.d.ts",
    "./node_modules/ng2-bs3-modal/dist/components/modal-body.d.ts",
    "./node_modules/ng2-bs3-modal/dist/components/modal-footer.d.ts",
    "./node_modules/ng2-bs3-modal/dist/components/modal-header.d.ts",
    "./node_modules/ng2-bs3-modal/dist/components/modal.d.ts",
    "./node_modules/ng2-bs3-modal/dist/ng2-bs3-modal.d.ts"
  ],
  "atom": {
    "rewriteTsconfig": true
  }
}
