{
  "name": "pciwake-web",
  "version": "1.0.0",
  "description": "An Angular2 client for the PCI Wake web application",
  "author": "tmeneau <tmeneau@xetus.com>",
  "license": "ISC",
  "main": "index.html",
  "scripts": {
    "build": "gulp build",
    "assetwatch": "gulp watch",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "start": "concurrent \"npm run assetwatch\" \"npm run tsc:w\" \"npm run lite\" ",
    "typings": "typings",
    "prestart": "npm run build",
    "postinstall": "find node_modules/ng2-bootstrap -name '*.ts' | grep -v '\\.d\\.ts$' | xargs rm"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.6",
    "bootstrap": "^3.3.6",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "ng2-bootstrap": "^1.0.3",
    "ng2-bs3-modal": "^0.1.3",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.0",
    "systemjs": "0.19.20",
    "zone.js": "0.5.14"
  },
  "devDependencies": {
    "concurrently": "^1.0.0",
    "del": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-plumber": "^1.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-typescript": "^2.11.0",
    "gulp-watch": "^4.3.5",
    "lite-server": "^2.0.1",
    "run-sequence": "^1.1.5",
    "typescript": "^1.7.5",
    "typings": "^0.6.8"
  }
}
